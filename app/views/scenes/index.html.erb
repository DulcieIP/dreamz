<div class="container">
  <h1> my scenes </h1>

  <div class="row">
    <% @scenes.each do |scene| %>
      <div class="scene-wrapper col-6 m-0 p-0">
        <%= cl_image_tag scene.image.key, class:"w-100 h-100", height:600, width:600, crop: :fill %>
        <i class="fa-regular fa-circle-check"></i>
      </div>
    <% end %>
  </div>
</div>
